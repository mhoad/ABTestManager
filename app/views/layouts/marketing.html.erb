<!DOCTYPE html>
<html>
  <head>
    <title>TestManager</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%= stylesheet_pack_tag 'shared', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_pack_tag 'marketing', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'shared', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'marketing', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <p class="notice"><%= notice %></p>
        <p class="alert"><%= alert %></p>
    <%= yield %>
  </body>
</html>